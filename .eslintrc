{
    "extends": [
        "react-app",
        "airbnb",
        "plugin:jsx-a11y/recommended",
        "prettier",
        "prettier/react"
    ],
    "plugins": [
        "jsx-a11y",
        "prettier"
    ],
    "rules": {
        "indent": ["error", 2],
        "eol-last": ["error", "never"],
        "comma-dangle": ["error", "never"],
        "no-param-reassign": ["error", {
            "props": false
        }],
        "semi": 0,
        "react/jsx-filename-extension": [1, {
            "extensions": [".js", ".jsx"]
        }],
        "prettier/prettier": [
            "error", {
                "semi": false
            }
        ]
    }
}